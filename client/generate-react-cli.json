{
  "usesTypeScript": true,
  "usesStyledComponents": true,
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/components/TemplateName.tsx",
        "style": "templates/components/TemplateName.module.scss",
        "index": "templates/components/index.ts",
        "model": "templates/components/TemplateName.model.ts"
      },
      "path": "src/app/components",
      "withStyle": true,
      "withTest": false,
      "withStory": true,
      "withLazy": false,
      "withIndex": true,
      "withModel": true
    },
    "slice": {
      "customTemplates": {
        "component": "templates/store/index.ts",
        "model": "templates/store/TemplateName.model.ts",
        "selector": "templates/store/TemplateName.selector.ts",
        "slice": "templates/store/TemplateName.slice.ts"
      },
      "path": "src/app/store",
      "withModel": true,
      "withSelector": true,
      "withSlice": true
    }
  }
}
